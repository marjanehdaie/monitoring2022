# R-code-ggplot2.r
# R code for ggplot2 based graphs
# install.packages("ggplot2")
# library(ggplot2)






virus <- c(10, 30, 40, 50, 60, 80)
death <- c(100, 240, 310, 470, 580, 690)
virus <- c(10, 30, 40, 50, 60, 80)
death <- c(100, 240, 310, 470, 580, 690)
data.frame(virus, death)
d <- data.frame(virus, death)
summary(d)
ggplot(d, aes(x=virus, y=death)) + geom-point()
ggplot(d, aes(x=virus, y=death)) + geom-point(size=3, col="coral", pch=17)

geom-line()
geom-polygon()


